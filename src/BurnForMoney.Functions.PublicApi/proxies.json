{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "strava_authorization_page_proxy": {
      "matchCondition": {
        "methods": [ "GET" ],
        "route": "/api/strava/authorize/"
      },
      "responseOverrides": {
        "response.statusCode": "302",
        "response.headers.location": "%StravaAppHostName%/api/strava/authorize"
      }
    }
  }
}